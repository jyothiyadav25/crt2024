
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {
     char str[50];
    scanf("%[^\n]s",str);
    int i,c=0;
    for(i=0;str[i]!='\0';i++)
    {
       
     if(str[i]=='a' || str[i]=='e' || str[i]=='i' || str[i]=='o' || str[i]=='u' ||str[i]=='A' || str[i]=='E' || str[i]=='I' ||str[i]=='O' || str[i]=='U')
           {
               c++;
           }
       }
    
    printf("%d",c);
      
    return 0;
}

