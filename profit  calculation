#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */   
    int x,a,b;
    scanf("%d%d%d",&x,&a,&b);
    int r1=x*a;
    int r2=x*b;
    int profit=r1-r2-100;
    printf("%d",profit);
    return 0;
}
