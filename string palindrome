
#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main() {

   char str[50],str1[50];
    scanf("%s",str);
    int n,i,j=0;
    n=strlen(str);
    for(i=n-1;i>=0;i--)
    {
        str1[j]=str[i];
        j++;
    }
    str1[j]='\0';
    i=strcmp(str,str1);
    if(i==0)
    {
        printf("Palindrome");
    }
    else
    {
        printf("Not a Palindrome");
    }
    return 0;
}
